    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2",
    "vite": "^5.0.0"
  },
  "type": "module",
  "dependencies": {
    "@sveltejs/adapter-static": "^3.0.0",
